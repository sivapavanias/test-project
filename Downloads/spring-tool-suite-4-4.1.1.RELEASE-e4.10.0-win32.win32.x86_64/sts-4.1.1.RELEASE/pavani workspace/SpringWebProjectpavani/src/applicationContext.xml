<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="emp" class="com.src.Employee">
<constructor-arg name="eid" value="111"></constructor-arg>
<constructor-arg name="name" value="SANJU*"></constructor-arg>
<constructor-arg name="address" >
<list>
<bean class="com.src.Address"> 
<constructor-arg name="hno" value="127"></constructor-arg>
<constructor-arg name="street" value="LA"></constructor-arg>
<constructor-arg name="city" value="HYDER"></constructor-arg>
</bean>
<bean class="com.src.Address"> 
<constructor-arg name="hno" value="131"></constructor-arg>
<constructor-arg name="street" value="VC"></constructor-arg>
<constructor-arg name="city" value="UPPAL"></constructor-arg>
</bean>
</list>
</constructor-arg>
</bean>
</beans>
